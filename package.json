{
  "name": "web-demo",
  "version": "2.0.0",
  "private": true,
  "description": "演示项目",
  "scripts": {
    "postinstall": "umi g tmp && run-script-os",
    "postinstall:win32": "cd scripts && blocks.cmd",
    "postinstall:darwin:linux": "sh ./scripts/blocks.sh",
    "postinstall:default": "sh ./scripts/blocks.sh",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "fetch:blocks": "pro fetch-blocks && npm run prettier",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "cross-env UMI_UI=none PORT=3000 umi dev",
    "dev": "npm run start:dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/compatible": "^1.0.8",
    "@ant-design/icons": "^4.5.0",
    "@ant-design/pro-list": "0.0.8",
    "@apollo/client": "^3.3.12",
    "@asany/umi-plugin-apollo": "^0.1.3",
    "@asany/umi-plugin-app": "^0.1.4",
    "@asany/components": "^0.1.1",
    "antd": "^4.13.0",
    "antd-img-crop": "^3.14.1",
    "better-xlsx": "^0.7.5",
    "blueimp-md5": "^2.18.0",
    "braft-editor": "^2.3.9",
    "braft-utils": "^3.0.12",
    "classnames": "~2.2.6",
    "echarts": "^5.0.2",
    "echarts-for-react": "^3.0.1",
    "file-saver": "^2.0.5",
    "image-conversion": "^2.1.1",
    "jquery": "^3.6.0",
    "overlayscrollbars-react": "^0.2.2",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.1",
    "react-cropper": "^2.1.7",
    "react-dnd": "^13.1.1",
    "react-dnd-html5-backend": "^12.1.1",
    "react-dom": "^17.0.1",
    "react-sortablejs": "^6.0.0",
    "react-use": "^15.3.8",
    "react-zmage": "^0.8.5-beta.36",
    "recoil": "^0.2.0",
    "umi": "^3.5.13"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.23",
    "@types/classnames": "^2.2.11",
    "@types/express": "^4.17.9",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/qrcode.react": "^1.0.1",
    "@types/qs": "^6.9.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@umijs/fabric": "^2.3.1",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/preset-react": "^1.7.0",
    "@umijs/preset-ui": "^2.2.9",
    "carlo": "^0.9.46",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "cross-port-killer": "^1.2.1",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "express": "^4.17.1",
    "husky": "^4.3.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.5.2",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "puppeteer-core": "^5.5.0",
    "raw-loader": "^4.0.2",
    "run-script-os": "^1.1.3",
    "stylelint": "^13.8.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ],
  "create-umi": {
    "ignoreScript": [
      "docker*",
      "functions*",
      "site",
      "generateMock"
    ],
    "ignoreDependencies": [
      "netlify*",
      "serverless"
    ],
    "ignore": [
      ".dockerignore",
      ".git",
      ".github",
      ".gitpod.yml",
      "CODE_OF_CONDUCT.md",
      "Dockerfile",
      "Dockerfile.*",
      "lambda",
      "LICENSE",
      "netlify.toml",
      "README.*.md",
      "azure-pipelines.yml",
      "docker",
      "CNAME",
      "create-umi"
    ]
  }
}
